$(function () {
    var curr_file = null;
    
    $(document).ready(function() {
	var img = new Image();
	img.src = "static/example.jpg";
	img.onload = function () {
	    var canvas = document.getElementById("myCanvas");
	    var context = canvas.getContext("2d");
	    context.drawImage(img, 0, 0, img.width/3, img.height/3);
	}
	
    });
});